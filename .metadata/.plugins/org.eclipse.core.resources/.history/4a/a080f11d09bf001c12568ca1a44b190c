package Assignments;

public class SortStudent 
{
	static Student[] sort(Student[] st)
	{
		for(int i = 0; i < st.length - 1; i++)
		{
			if(st[i].getRollNo()>st[i+1].getRollNo())
			{
				Student temp = st[i];
				st[i] = st[i+1];
				st[i+1] = temp;
			}
		}
		return st;
	}
	public static void main(String[] args) 
	{
		Student st1 = new Student(101, "Raju");
		Student st2 = new Student(103, "Adi");
		Student st3 = new Student(102, "Ganesh");
		Student[] st = {st1, st2, st3}; 
		
		for(int i = 0; i < st.length; i++)
		{
			System.out.println(st[i].getRollNo());
		}
		
		sort(st);
		
		System.out.println("----------");
		for(int i = 0; i < st.length; i++)
		{
			System.out.println(st[i].getRollNo());
		}
	}
}
