package Assignmentss;

public class PizzaOrder 
{
	public void calculateTotalOrderCost() 
	{
        double totalOrderCost = 0.0;
        for (Pizza pizza : pizzas) 
        {
            switch (pizza.getName()) 
            {
                case "pizza1":
                    totalOrderCost += pizza1;
                    break;
                case "pizza2":
                    totalOrderCost += pizza2;
                    break;
                case "pizza3":
                    totalOrderCost += pizza3;
                    break;
                default:
                    totalOrderCost += 0.0;
                    break;
            }
        }
	}
	public static void main(String[] args) 
	{
		Pizza p1 = new Pizza("pizza1", 99.99);
		Pizza p2 = new Pizza("pizza1", 89.99);
		Pizza p3 = new Pizza("pizza1", 79.99);
		
	}
}
