package Assignments;

public class Addition 
{
	private int a;
	private int b;
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	public static void main(String[] args) 
	{
		Addition addition=new Addition();
		addition.a=10;
		System.out.println(addition.a);
	}
}
