package Assignments;

public class Student 
{
	int marks;
	String name;
	Student(String name, int marks)
	{
		this.marks = marks;
		this.name = name;
	}
	String getGrade()
	{
		if(marks>100)
			return "Invalid Marks";
		if(marks>=90)
			return name + " got Grade - A";
		if(marks>=80)
			return name + " got Grade - B";
		if(marks>=70)
			return name + " got Grade - C";
		return name +" is Fail";
	}
}
